{
    "manifest_version" : 2,
    "name" : "Cookie Eater",
    "version" : "0.1.0",
    "browser_action" : {
        "default_popup" : "sketch/popup.html",
        "default_icon" : "icon.png"
    },
    "icons":{
        "128" : "icon.png"
    },
    "background" : {
        "scripts" : ["background.js"]
    },
    "content_scripts" : [
        {
            "matches":["<all_urls>"],
            "js" : ["script.js"],
            "css":["style.css"]
        }
    ]
}