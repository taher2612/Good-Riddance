{
    "manifest_version" : 2,
    "name" : "Good Riddance",
    "version" : "1.0.0",
    "description": "Increase productivity through speedy disposal of unwanted content, with just few clicks.",
    "browser_action" : {
        "default_popup" : "sketch/popup.html",
        "default_icon" : "icon6.png"
    },
    "icons":{
        "128" : "icon6.png"
    },
    "content_scripts" : [
        {
            "matches":["<all_urls>"],
            "js" : ["script.js"],
            "css":["style.css"]
        }
    ]
}